#!/usr/bin/env node

function banner() {
  console.log("Tempo: A node benchmarking tool.");
  console.log("Usage: nb <cpu> <clients> <requests> <yaml file>");
  console.log("Options:");
  console.log("  cpu:      Number of cpus to utilize (defaults to 1)");
  console.log("  clients:  Number of clients per cpu (defaults to 1)");
  console.log("  requests: Number of requests per client (defaults to 100)");
}


var args = process.argv;

var file     = args.pop();
var requests = args.pop();
var clients  = args.pop();
var cpus     = args.pop();


banner();

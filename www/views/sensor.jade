extend layout
block content
  h5
    a(href="/") services 
    = ' > '
    a(href="/services/#{params.service}")= params.service 
    = ' > ' + params.sensor
  .chart(data-url="/services/#{params.service}/sensors/#{params.sensor}.json", data-type="pass-fail")
  style
    .chart div { height: 300px }
